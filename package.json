{
  "name": "predictscore-ml",
  "version": "1.0.0",
  "description": "Sistema Predictivo de Rendimiento Académico con Machine Learning",
  "author": "Grupo 4 - Universidad Nacional de Cañete",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && python main_simple.py",
    "frontend": "cd frontend && npm run dev",
    "install-all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "test": "python scripts/validate_system.py",
    "build": "cd frontend && npm run build",
    "start": "npm run dev"
  },
  "keywords": [
    "machine-learning",
    "education",
    "prediction",
    "academic-performance",
    "svr",
    "fastapi",
    "nextjs",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/[username]/PredictScore-ML.git"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
